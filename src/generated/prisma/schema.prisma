generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id        Int    @id @default(autoincrement())
  nome      String
  email     String @unique
  senhaHash String
  tipo      String
}
